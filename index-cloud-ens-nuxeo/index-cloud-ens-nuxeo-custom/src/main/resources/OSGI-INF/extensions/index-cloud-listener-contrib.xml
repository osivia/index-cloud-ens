<?xml version="1.0"?>
<component
	name="fr.index.cloud.ens.metadata.listener.contrib" version="1.0">

	<require>org.nuxeo.ecm.core.event.EventServiceComponent</require>

	<extension
		target="org.nuxeo.ecm.core.event.EventServiceComponent"
		point="listener">

		<listener name="indexDuplicationListener" async="false"
			postCommit="false" enabled="true"
			class="fr.index.cloud.ens.metadata.DuplicationListener"
			priority="10">
			<event>documentCreatedByCopy</event>			
		</listener>

	</extension>




</component>